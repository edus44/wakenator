        "list": {
          ".read": true,
          "$uid": {
            ".write": "$uid === auth.uid",
            ".validate": "newData.child('name').isString()"
          }
        },
        "wakes": {
          "$uid": {
            ".read": "$uid === auth.uid",
            "$id": {
              ".write": "$uid === auth.uid ||Â newData.child('uid').val() === auth.uid",
              ".validate": "newData.child('name').isString()"
            }
          }
        }
      }
    }
  }
}
